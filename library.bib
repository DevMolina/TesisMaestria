  file      = {:C$\backslash$:/Users/henry/OneDrive/Escritorio/Maestria/estado del arte/yu2020.pdf:pdf},
  issn      = {09253467},
  journal   = {Opt. Mater. (Amst).},
  keywords  = {(Gd,Y)AG:Mn2+,Ce3+ phosphor,High temperature solid phase method,Luminescent property},
  number    = {October},
  pages     = {110566},
  publisher = {Elsevier B.V.},
  title     = {{Synthesis, luminescent properties and crystal stabilization of \ce{GdAG}:\ce{Mn^{2+}/Ce^{3+}} via \ce{Y^{3+}} doping for warm w-LED application}},
