  journal   = {Ceram. Int.},
  keywords  = {Atmospheric pressure sintering,Ceramic phosphor,Rare earth luminescence,Transition metal luminescence,White LED},
  number    = {7},
  pages     = {9156--9163},
  publisher = {Elsevier Ltd},
  title     = {{Synthesis and luminescence properties of color-tunable Ce, Mn co-doped LuAG transparent ceramics by sintering under atmospheric pressure}},
