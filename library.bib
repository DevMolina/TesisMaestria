  title           = {{Electronic structure and optical properties of the lanthanide activated \ce{RE3(Al_{1-x}Ga_{x})_{5}O12} (RE=\ce{Gd}, \ce{Y}, \c{Lu}) garnet compounds}},
  url             = {http://dx.doi.org/10.1016/j.jlumin.2012.08.028},
  volume          = {134},
