  year      = {2021}
}

@article{Manimuthu2015,
  abstract  = {Materials exhibiting low-field magneto-electric and magneto-dielectric (MD) response at room temperature are essential for utilization in multifunctional devices. Magneto-dielectric properties of Lu3Fe5O12 substituted with a magnetic ion (Terbium) at its dodecahedral (non-magnetic) site are investigated. The measured dielectric permittivity at various magnetic fields exhibits a giant MD effect ({\~{}}65{\%}) at remarkably low fields (H{\~{}}1 kOe) at room temperature. The MD response of Lu3Fe5O12 is enhanced due to the coupling effect of substituted magnetic Terbium ions in the non-magnetic Lu-site interacting with the neighboring Fe-sites. Studies suggest a route towards future applications of the MD effect in advanced devices.},
